@model Assignment_C_4.Models.NguoiDung

@{
    ViewData["Title"] = "CreateND";
}

<h1>Đăng Ký</h1>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="CreateND">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="TaiKhoan" class="control-label"></label>
                <input asp-for="TaiKhoan" class="form-control" />
                <span asp-validation-for="TaiKhoan" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="MatKhau" class="control-label"></label>
                <input asp-for="MatKhau" class="form-control" />
                <span asp-validation-for="MatKhau" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TenND" class="control-label"></label>
                <input asp-for="TenND" class="form-control" />
                <span asp-validation-for="TenND" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="SDT" class="control-label"></label>
                <input asp-for="SDT" class="form-control" />
                <span asp-validation-for="SDT" class="text-danger"></span>
            </div>
            <br />
            <div class="form-group">
                <input type="submit" value="Đăng Ký" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>


